{
  "name": "twitch-bot-server",
  "version": "1.0.0",
  "private": true,
  "description": "Twitch bot server!",
  "author": "Michael Webb",
  "license": "MIT",
  "homepage": "https://github.com/mjfwebb/twitch-bot",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm run build:live",
    "start-test": "cross-env NODE_ENV=test npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch \"src/**/*.ts\" --exec \"ts-node\" src/index.ts",
    "lint": "eslint -c .eslintrc.js --ext .ts src/",
    "test": "jest",
    "dev": "nodemon -L src/index.ts",
    "setup": "node ./setup.mjs"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "keywords": [],
  "dependencies": {
    "@inquirer/checkbox": "^1.3.1",
    "@inquirer/confirm": "^2.0.2",
    "@inquirer/prompts": "^2.1.1",
    "console-stamp": "^3.0.2",
    "cors": "^2.8.5",
    "discord.js": "^14.11.0",
    "dotenv": "^8.2.0",
    "ffmpeg-static": "^5.1.0",
    "http-status-codes": "^2.2.0",
    "mongodb": "^4.13.0",
    "mongoose": "^6.8.3",
    "node-fetch": "^2.6.7",
    "octokit": "^2.0.14",
    "open": "^8.4.0",
    "picocolors": "^1.0.0",
    "play-sound": "^1.1.5",
    "socket.io": "^4.5.4",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@swc/core": "^1.3.26",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.2.5",
    "@types/node": "^13.13.45",
    "@types/node-fetch": "^2.6.2",
    "@types/play-sound": "^1.1.0",
    "@types/websocket": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^8.10.2",
    "typescript": "^4.8.2"
  }
}
